<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="812.0" prefWidth="1124.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="picpro.ProcessImageController">
   <children>
      <BorderPane layoutX="4.0" layoutY="12.0" prefHeight="793.0" prefWidth="1107.0">
         <top>
            <Label prefHeight="61.0" prefWidth="155.0" text="Image Processor" BorderPane.alignment="CENTER">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
         </top>
         <bottom>
            <ButtonBar prefHeight="61.0" prefWidth="1007.0" BorderPane.alignment="CENTER">
               <buttons>
                  <Button fx:id="processButton" mnemonicParsing="false" onMouseClicked="#selectButton" prefHeight="34.0" prefWidth="86.0" text="Process" translateX="-300.0" />
                  <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#backButton" prefHeight="35.0" text="Back" translateX="-700.0" />
                  <Button fx:id="quitButton" mnemonicParsing="false" onMouseClicked="#quitButton" prefHeight="33.0" text="Quit" translateX="-175.0" />
               </buttons>
            </ButtonBar>
         </bottom>
         <center>
            <Pane prefHeight="671.0" prefWidth="1060.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="browseField" layoutX="228.0" layoutY="627.0" prefHeight="26.0" prefWidth="546.0" />
                  <Button fx:id="browseButton" layoutX="871.0" layoutY="620.0" mnemonicParsing="false" onMouseClicked="#browseButton" prefHeight="40.0" prefWidth="83.0" text="Browse" textFill="#a80a0a" />
                  <ImageView fx:id="imageSlot" fitHeight="589.0" fitWidth="753.0" layoutX="228.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@picPro.png" />
                     </image></ImageView>
                  <Button fx:id="incrementPhotosUp" layoutX="1019.0" layoutY="288.0" mnemonicParsing="false" onMouseClicked="#incrementPhotosUp" text="--&gt;" />
                  <Button fx:id="incrementPhotosDown" layoutX="56.0" layoutY="301.0" mnemonicParsing="false" onMouseClicked="#incrementPhotosDown" text="&lt;--" />
               </children>
            </Pane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
